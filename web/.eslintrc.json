{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope":"off",
    "react/jsx-props-no-spreading": "off",
    "import/prefer-default-export": "off",
    "eslint jsx-a11y/label-has-associated-control": [
      "error",
      { "assert": "either" }
    ]
  }
}
